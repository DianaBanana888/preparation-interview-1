<div id="wrap-deck-create" class="row">
  <form action="POST" class="col s12" id="form-deck-create">
    <div class="input-field col s12">
      <input class="data" required placeholder="Укажите название вашего опросника" name="title" type="text">
    </div>

    <div class="questions-wrapper">

    </div>

    <div class="submit-wraper">
      <button type="submit" class="waves-effect waves-light btn-large blue-grey darken-2 disabled submit-deck">
        <i class="material-icons left">cloud</i>Отправить
      </button>
    </div>
  </form>
  <div class="add-input-deck">
    <div>Добавить поле</div>
    <a class="btn-floating btn-large waves-effect create-field">
      <i class="material-icons">add</i>
    </a>
  </div>


</div>
